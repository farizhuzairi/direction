{
    "name": "farizhuzairi/direction",
    "type": "library",
    "description": "Layanan manajemen akses dan sumber daya khusus untuk aplikasi dengan dukungan (lisensi) EMS Platform.",
    "keywords": [
        "direction service",
        "ems platform",
        "entriplus",
        "hascha digital media"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/farizhuzairi/direction/issues",
        "source": "https://github.com/farizhuzairi/direction"
    },
    "authors": [
        {
            "name": "Fariz Huzairi",
            "email": "farizhuzairi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "hascamp/basecrypt": "^1.0",
        "league/commonmark": "^2.6@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.3"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Director\\": "src/"
        }
    },
    
    "extra": {
        "laravel": {
